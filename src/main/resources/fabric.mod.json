{
  "schemaVersion": 1,
  "id": "quicksave",
  "version": "${mod_version}",

  "name": "Quicksave",
  "description": "Quicksaving! In Minecraft! Quickly save a backup and reload from it in-game.",
  "authors": [
    "5HT2"
  ],
  "contact": {
    "homepage": "https://frogg.ie/",
    "sources": "https://github.com/5HT2/fabric-quicksave/"
  },

  "license": "OSL-3.0",
  "icon": "assets/quicksave/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.fabricmc.example.ExampleMod"
      }
    ]
  },
  "mixins": [
    "quicksave.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.2+kotlin.1.8.10",

  },
  "suggests": {
    "another-mod": "*"
  }
}
